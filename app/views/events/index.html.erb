<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Date</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.title %></td>
        <td><%= event.start_date %></td>
        <td>
          <%= link_to "View", event_path(event) %>
          <%= link_to "Edit", edit_admin_event_path(event) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
